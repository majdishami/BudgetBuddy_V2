{
  "name": "budgetbuddy",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "client"
  ],
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm run build --workspace=client",
