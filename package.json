    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.41.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.484.0",
    "pg": "^8.14.1",
    "tailwind-merge": "^3.0.2",
